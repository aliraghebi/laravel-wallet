{
  "name": "arsamme/laravel-wallet",
  "description": "Virtual wallet implementation to use in Laravel projects.",
  "keywords": [
    "laravel",
    "credits",
    "arsam",
    "currency",
    "laravel-wallet",
    "laravel-package",
    "virtual",
    "wallet"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/arsamme/laravel-wallet",
  "license": "MIT",
  "authors": [
    {
      "name": "Arsam",
      "email": "info@arsam.me"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "brick/math": "~0.10",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "laravel/framework": "^10.0|^11.0|^12.0",
    "laravel/pint": "^1.22",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ArsamMe\\Wallet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArsamMe\\Wallet\\Test\\": "tests/"
    }
  },
  "scripts": {
    "parabench": "@php ./vendor/bin/testbench package:test --coverage-xml=build/coverage-xml --log-junit=build/junit.xml"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArsamMe\\Wallet\\LaravelWalletServiceProvider"
      ],
      "aliases": {
        "LaravelWallet": "ArsamMe\\Wallet\\Facades\\LaravelWallet"
      }
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
